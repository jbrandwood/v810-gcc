@@ -0,0 +1,27 @@
+    .section .text
+    .align  1
+    .global _memmove
+    .type   _memmove, @function
+_memmove:
+    mov     r6, r10
+    # save bitstring registers
+    mov     r26, r11
+    mov     r27, r12
+    mov     r28, r13
+    mov     r29, r14
+    # move bitstring
+    mov     r6, r29
+    mov     r7, r30
+    shl     3, r6
+    mov     r6, r26
+    shl     3, r7
+    mov     r7, r27
+    shl     3, r8
+    mov     r8, r28
+    movbsu
+    # restore bitstring registers
+    mov     r11, r26
+    mov     r12, r27
+    mov     r13, r28
+    mov     r14, r29
+    jmp     [r31]
